{% extends admin() %}

{% block content %}
    <h1>Custom Field</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ entity.code }}</td>
            </tr>
            <tr>
                <th>Datatype</th>
                <td>{{ entity.datatype }}</td>
            </tr>
            <tr>
                <th>Is_displayed</th>
                <td>{{ entity.isdisplayed }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('cart_admin_item_var') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('cart_admin_item_var_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
