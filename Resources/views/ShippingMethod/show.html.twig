{% extends 'MobileCartAdminBundle::admin-layout.html.twig' %}

{% block content %}
    <h1>ShippingMethod</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Method</th>
                <td>{{ entity.method }}</td>
            </tr>
            <tr>
                <th>Min_days</th>
                <td>{{ entity.mindays }}</td>
            </tr>
            <tr>
                <th>Max_days</th>
                <td>{{ entity.maxdays }}</td>
            </tr>
            <tr>
                <th>Is_taxable</th>
                <td>{{ entity.istaxable }}</td>
            </tr>
            <tr>
                <th>Is_discountable</th>
                <td>{{ entity.isdiscountable }}</td>
            </tr>
            <tr>
                <th>Is_price_dynamic</th>
                <td>{{ entity.ispricedynamic }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ entity.price }}</td>
            </tr>
            <tr>
                <th>Pre_conditions</th>
                <td>{{ entity.preconditions }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('cart_admin_shippingmethod') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('cart_admin_shippingmethod_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
