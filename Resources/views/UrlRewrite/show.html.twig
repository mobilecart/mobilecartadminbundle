{% extends 'MobileCartAdminBundle::admin-layout.html.twig' %}

{% block content %}
    <h1>UrlRewrite</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Object Type</th>
                <td>{{ entity.objecttype }}</td>
            </tr>
            <tr>
                <th>Object Action</th>
                <td>{{ entity.objectaction }}</td>
            </tr>
            <tr>
                <th>Request URI</th>
                <td>{{ entity.requesturi }}</td>
            </tr>
            <tr>
                <th>Is Redirect</th>
                <td>{{ entity.isredirect }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('cart_admin_url_rewrite') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('cart_admin_url_rewrite_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
