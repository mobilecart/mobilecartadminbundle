{% extends 'MobileCartCoreBundle::admin-layout.html.twig' %}

{% block content %}
    <h1>Order</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Json</th>
                <td>{{ entity.json }}</td>
            </tr>
            <tr>
                <th>Total</th>
                <td>{{ entity.total }}</td>
            </tr>
            <tr>
                <th>Item_total</th>
                <td>{{ entity.itemtotal }}</td>
            </tr>
            <tr>
                <th>Tax_total</th>
                <td>{{ entity.taxtotal }}</td>
            </tr>
            <tr>
                <th>Discount_total</th>
                <td>{{ entity.discounttotal }}</td>
            </tr>
            <tr>
                <th>Shipping_total</th>
                <td>{{ entity.shippingtotal }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('cart_admin_order') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('cart_admin_order_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
